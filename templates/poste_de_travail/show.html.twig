




{% extends 'base.html.twig' %}

{% block title %}poste de travail{% endblock %}

{% block body %}

    <div class="wrapper site-min-height">
        <!-- page start-->
        <div class="chat-room mt">
            <div class="col-lg-offset-2">
                <aside class="mid-side">
                    <div class="chat-room-head">
                        <div class="centered">
                        <h1> le poste </h1>
                        </div>
                        <div class="form-group">
                            <div class="pull-right col-lg-1">
                                <button type="button" class="btn btn-primary  btn-dark btn-lg color-block mb-1 mx-auto z-depth-1 btn-rounded ">
                                    <a href="{{ path('poste_de_travail_index') }}" class="text-white" title="retour Ã  la liste"><i class="fa fa-reply"></i></a></button>

                            </div>

                        </div>
                        <br>


                    </div>



                    <div class="group-rom">
                        <div class="first-part odd">id</div>
                        <div class="second-part">{{ poste_de_travail.id }}</div>

                    </div>

                    <div class="group-rom">
                        <div class="first-part odd">Nom<i class="fa fa-calculator" aria-hidden="true"></i></div>
                        <div class="second-part">{{ poste_de_travail.nom }}</div>
                    </div>

                    <div class="group-rom">
                        <div class="first-part odd">Refrence</div>
                        <div class="second-part">{{ poste_de_travail.refrence }}</div>

                    </div>
                    <div class="group-rom">
                        <div class="first-part odd">Refrence</div>
                        <div class="second-part">{{ poste_de_travail.refrence }}</div>

                    </div>
                    <div class="group-rom">
                        <div class="first-part odd">Horaire</div>
                        <div class="second-part">{{ poste_de_travail.horaire }}</div>

                    </div>
                    <div class="group-rom">
                        <div class="first-part odd">Lieu</div>
                        <div class="second-part">{{ poste_de_travail.lieu }}</div>

                    </div>


                    <div class="group-rom">
                    <div class="first-part odd">Atelier</div>
                    <div class="second-part">{{ poste_de_travail.atelier }}</div>

                    </div>
                    <div class="group-rom">
                        <div class="first-part odd">Exigence</div>
                        <div class="second-part">{{ poste_de_travail.exigence}}</div>

                    </div>
                    <div class="group-rom">
                        <div class="first-part odd">Moyen de maitise</div>
                        <div class="second-part">{{ poste_de_travail.moyenmaitrise}}</div>

                    </div>
                    <div class="group-rom">
                        <div class="first-part odd">Equipement</div>
                        <div class="second-part"><ul>
                                {% for i in poste_de_travail.equipement %}
                                    <li>{{ i }}</li>
                                {% endfor %}
                            </ul></div>

                    </div>









                </aside>

            </div>
        </div>
    </div>



{% endblock %}









